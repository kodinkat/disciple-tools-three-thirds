  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "npx kill-port 3000 && npx kill-port 3001 && mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "npm run foundation && mix --production",
    "foundation": "sass magic-link/styles/foundation.scss dist/foundation.css --style compressed",
    "watch:foundation": "sass magic-link/styles/foundation.scss dist/foundation.css --watch"
  },
  "repository": {
